{
  "manifest_version": "1",
  "provider": {
    "name": "AQRAG",
    "type": "intelligent_memory",
    "version": "1.0.0"
  },
  "capabilities": {
    "core_operations": {
      "add_memory": true,
      "retrieve_memory": true,
      "delete_memory": true
    },
    "optional_operations": {
      "update_memory": false,
      "list_memories": true,
      "reset_scope": true,
      "get_capabilities": false
    },
    "system_flags": {
      "async_indexing": true,
      "processing_latency": 300,
      "convergence_wait_ms": 750
    },
    "intelligence_flags": {
      "auto_extraction": true,
      "graph_support": false
    }
  },
  "semantic_properties": {
    "update_strategy": "eventual",
    "delete_strategy": "immediate"
  },
  "conformance_tests": {
    "expected_behavior": {
      "convergence_wait_ms": 750
    }
  }
}
